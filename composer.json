{
  "name": "encreinformatique/silex-rss-provider",
  "description": "Simple provider to generate a rss feed.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "silex",
    "rss",
    "feed",
    "xml"
  ],
  "authors": [
    {
      "name": "Julien Devergnies",
      "email": "j.devergnies@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EncreInformatique\\SilexRssProvider\\": "src/"
    }
  },
  "require": {
    "php": ">=5.6.0",
    "silex/silex": "^2.0"
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "tools": {
      "behat": {
        "url": "https://github.com/Behat/Behat/releases/download/v3.2.1/behat.phar"
      }
    }
  }
}
